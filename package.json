{
  "name": "webpack-express-pug-boilerplate",
  "version": "0.1.0",
  "main": "server.js",
  "author": "Katherine Ebel",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run build:server && node server.js",
    "build:server": "babel server.babel.js -o server.js",
    "build:prod": "NODE_ENV=production webpack -p --config webpack.prod.config.js --progress",
    "build": "better-npm-run build",
    "clean": "rimraf dist/* && rimraf server.js",
    "debug": "node --inspect-brk $(npm bin)/webpack --verbose",
    "jscs": "jscs ."
  },
  "betterScripts": {
    "build": {
      "command": "npm run clean && npm run build:server && npm run build:prod",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jscs": "^3.0.0-beta1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "chai": "^4.1.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jscs": "^3.0.7",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "standard": "^10.0.3",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.5.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "express": "^4.15.4",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2"
  },
  "standard": {
    "globals": [
      "$",
      "_"
    ]
  }
}
